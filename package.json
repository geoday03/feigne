    "axios": "^0.27.2",
    "next": "12.2.0",
    "next-pwa": "^5.5.4",
    "next-seo": "^5.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-redux": "^8.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/cz-commitlint": "^17.2.0",
    "@types/react": "^18.0.24",
    "commitizen": "^4.2.4",
    "commitlint": "^17.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "^12.3.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sznm": "^1.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  }
